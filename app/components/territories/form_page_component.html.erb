<%= render breadcrumb %>

<%= simple_form_for(territory, url: target_url, as: :territory) do |f| %>
  <%= input_wrapper do %>
    <%= f.input :name %>
  <% end %>
  <%= input_wrapper do %>
    <%= f.input :initial_phone_number %>
  <% end %>
  <%= input_wrapper do %>
    <%= f.input :final_phone_number %>
  <% end %>
  <%= input_wrapper do %>
    <%= f.submit(class: 'btn btn-primary mt-3 float-right') %>
  <% end %>
<% end %>
